{
  "name": "@logux/eslint-config",
  "version": "45.0.0",
  "description": "An ESLint shareable config for Logux",
  "keywords": [
    "logux",
    "eslint",
    "eslint-config"
  ],
  "scripts": {
    "test": "eslint . && ./check-peer.js && yaspeller *.md"
  },
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "logux/eslint-config",
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "globals": "^13.6.0"
  },
  "peerDependencies": {
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^1.1.0",
    "eslint-plugin-prettierx": "^0.17.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^28.0.2"
  },
  "devDependencies": {
    "@logux/sharec-config": "^0.8.5",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "clean-publish": "^1.1.9",
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^1.1.0",
    "eslint-plugin-prettierx": "^0.17.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^28.0.2",
    "lint-staged": "^10.5.4",
    "simple-pre-commit": "^1.2.1",
    "typescript": "^4.2.2",
    "yaspeller": "^7.0.0"
  },
  "simple-pre-commit": "npx lint-staged",
  "lint-staged": {
    "*.md": "yaspeller",
    "*.js": "eslint --fix",
    "package.json": "./check-peer.js"
  },
  "eslintConfig": {
    "extends": "./ts"
  },
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "ignoreText": [
      " \\(by [^)]+\\)."
    ],
    "dictionary": [
      "Logux ESLint",
      "ESLint",
      "Logux",
      "Versioning",
      "ES2015",
      "js",
      "configs",
      "ES6",
      "JSX"
    ]
  },
  "sharec": {
    "config": "@logux/sharec-config",
    "version": "0.8.5"
  }
}
