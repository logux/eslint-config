{
  "name": "@logux/eslint-config",
  "version": "54.5.0",
  "description": "An ESLint shareable config for Logux",
  "keywords": [
    "logux",
    "eslint",
    "eslint-config"
  ],
  "scripts": {
    "test:lint": "eslint .",
    "test:demo": "node demo/test.js",
    "test": "pnpm run /^test:/"
  },
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "logux/eslint-config",
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./ts": "./ts.js",
    "./svelte": "./svelte.js",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-perfectionist": "^4.9.0",
    "eslint-plugin-prefer-let": "^4.0.0",
    "neostandard": "^0.12.1",
    "typescript-eslint": "^8.24.1"
  },
  "peerDependencies": {
    "eslint": "^8.57.0 || ^9.0.0",
    "eslint-plugin-svelte": "^3.0.0-next.18",
    "svelte": "^4.2.12 || ^5.0.0",
    "svelte-eslint-parser": "^1.0.0-next.13"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-svelte": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    }
  },
  "devDependencies": {
    "clean-publish": "^5.1.0",
    "eslint": "^9.20.1",
    "eslint-plugin-svelte": "^3.0.0-next.18",
    "svelte": "^5.20.2",
    "svelte-eslint-parser": "^1.0.0-next.13",
    "typescript": "^5.7.3"
  },
  "prettier": {
    "arrowParens": "avoid",
    "jsxSingleQuote": false,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "clean-publish": {
    "cleanDocs": true
  }
}
